<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:name=".config.EquipoApp"
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/ic_mycoach_2"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_mycoach_2"
        android:supportsRtl="true"
        android:theme="@style/Theme.MyCoachBasketball"
        tools:targetApi="31">
        <activity
            android:name=".ui.AddTeamActivity"
            android:exported="false"
            android:label="@string/title_activity_add_team"
            android:theme="@style/Theme.MyCoachBasketball" />
        <activity
            android:name=".view.TeamsActivity"
            android:exported="false" />
        <activity
            android:name=".view.HomePage"
            android:exported="false" />
        <activity
            android:name=".view.RegisterPage"
            android:exported="false" />
        <activity
            android:name=".view.MainActivity"
            android:exported="true">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>